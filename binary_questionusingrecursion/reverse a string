
// Online C++ compiler to run C++ program online
#include <iostream>
 using namespace std;
   void reverse(char  *arr, int s, int e){
       if(s>=e){
           return ;
       }
       swap(arr[s], arr[e]);
       reverse(arr, ++s, --e);
   }
 int main(){
     char name[6]="arpan";
     reverse(name, 0,5);
     cout<< name;
     /*for(int i=0; i<5; i++){
         cout
     }*/
     return 0;
 }
